<!--     CONTRACT OPTION-->

     <xs:element name="ContractOptionRequest">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="player" type="tns:playerIDType"/>
             </xs:sequence>
         </xs:complexType>
     </xs:element>
     <xs:element name="ExistDBContractOptionRequest">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="player" type="tns:playerIDType"/>
             </xs:sequence>
         </xs:complexType>
     </xs:element>
     <xs:complexType name="playerIDType">
         <xs:attribute name="playerID" type="xs:int" use="required"/>
     </xs:complexType>



     <xs:element name="ContractOptionResponse">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="contractOption" type="xs:string"/>
             </xs:sequence>
         </xs:complexType>
     </xs:element>

     <!--     CONTRACT OPTION-->

<!--      PLAYERS-->
     <xs:element name="Players">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="players" type="tns:playerType"  maxOccurs="unbounded"/>
             </xs:sequence>
         </xs:complexType>
     </xs:element>
     <xs:complexType name="playerType">
             <xs:sequence>
                 <xs:element name="fullName" type="xs:string"/>
                 <xs:element name="dateOfBirth" type="xs:date"/>
                 <xs:element name="age" type="xs:int"/>
                 <xs:element name="height" type="xs:double"/>
                 <xs:element name="citizenship" type="xs:string"/>
                 <xs:element name="position" type="xs:string"/>
                 <xs:element name="foot" type="xs:string"/>
                 <xs:element name="outfitter" type="xs:string"/>
             </xs:sequence>
             <xs:attribute type="xs:int" name="contractID" use="optional"/>
             <xs:attribute type="xs:int" name="id" use="optional"/>
         </xs:complexType>

     <xs:element name="PlayersRequest">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="request" type="xs:string"/>
             </xs:sequence>
         </xs:complexType>
     </xs:element>

     <xs:element name="ExistDBPlayersRequest">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="request" type="xs:string"/>
             </xs:sequence>
         </xs:complexType>
     </xs:element>
<!--      PLAYERS-->

<!--     PlayersByCF-->

     <xs:element name="PlayersByCFRequest">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="citizenship" type="xs:string"/>
                 <xs:element name="foot" type="xs:string"/>
             </xs:sequence>
         </xs:complexType>
     </xs:element>
     <xs:element name="ExistDBPlayersByCFRequest">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="citizenship" type="xs:string"/>
                 <xs:element name="foot" type="xs:string"/>
             </xs:sequence>
         </xs:complexType>
     </xs:element>
     <!--     PlayersByCF-->

     <!--     PlayersByN-->

     <xs:element name="PlayersByNRequest">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="fullName" type="xs:string"/>
             </xs:sequence>
         </xs:complexType>
     </xs:element>

     <xs:element name="ExistDBPlayersByNRequest">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="fullName" type="xs:string"/>
             </xs:sequence>
         </xs:complexType>
     </xs:element>
     <!--     PlayersByCF-->

<!--    Server Response-->
     <xs:element name="ServerResponse">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="status" type="xs:string"/>
             </xs:sequence>
         </xs:complexType>
     </xs:element>
     <!--    Server Response-->

<!--     Remove player Request-->
     <xs:element name="RemovePlayerRequest">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="player" type="tns:playerIDType"/>
             </xs:sequence>
         </xs:complexType>
     </xs:element>

     <!--     Remove player Request-->
     <xs:element name="ExistDBRemovePlayerRequest">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="player" type="tns:playerIDType"/>
             </xs:sequence>
         </xs:complexType>
     </xs:element>


<!--     UpdateContract-->
     <xs:element name="UpdateContract">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="joined" type="xs:date"/>
                 <xs:element name="contractExpires" type="xs:date"/>
                 <xs:element name="contractOption" type="xs:string"/>
             </xs:sequence>
             <xs:attribute type="xs:string" name="currentClubID" use="optional"/>
             <xs:attribute type="xs:string" name="id" use="optional"/>
             <xs:attribute type="xs:string" name="previousClubID" use="optional"/>
         </xs:complexType>
     </xs:element>
<!--     UpdateContract-->

<!--     Update manager-->
     <xs:element name="UpdateManager">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="managerName" type="xs:string"/>
             </xs:sequence>
             <xs:attribute type="xs:int" name="id" use="required"/>
         </xs:complexType>
     </xs:element>
<!--     Update manager-->


     <!--Get managers-->
     <xs:element name="Managers">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="managers" type="tns:managerType"  maxOccurs="unbounded"/>
             </xs:sequence>
         </xs:complexType>
     </xs:element>
     <xs:complexType name="managerType">
         <xs:sequence>
             <xs:element name="managerName" type="xs:string"></xs:element>
         </xs:sequence>
         <xs:attribute name="id" type="xs:int" use="required"/>
     </xs:complexType>
     <!--Get managers-->



     <xs:element name="ManagersRequest">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="request" type="xs:string"/>
             </xs:sequence>
         </xs:complexType>
     </xs:element>

     <xs:element name="ExistDBManagersRequest">
         <xs:complexType>
             <xs:sequence>
                 <xs:element name="request" type="xs:string"/>
             </xs:sequence>
         </xs:complexType>
     </xs:element>

 </xs:schema>