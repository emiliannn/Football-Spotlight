        <xs:element name="player">
            <xs:complexType>
                <xs:sequence>
                    <xs:element name="fullName" type="xs:string"/>
                    <xs:element name="dateOfBirth" type="xs:date"/>
                    <xs:element name="age" type="xs:int"/>
                    <xs:element name="height" type="xs:double"/>
